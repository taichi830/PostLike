
# platform :ios, '14.1'


  # Pods for postLike

 def install_pods
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'FirebaseFirestore', :git => 'https://github.com/invertase/firestore-ios-sdk-frameworks.git', :tag => '8.1.0'
  pod 'Firebase/Storage'
  pod 'Firebase/Analytics'
  pod 'Firebase/Messaging'
  pod 'SDWebImage'
  pod 'DKImagePickerController' 
  pod 'InstantSearchClient'
  pod 'Firebase/DynamicLinks'
  pod 'Firebase/AdMob'
#  pod 'Google-Mobile-Ads-SDK'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'GoogleSignIn'

 end
 

 target 'PostLike' do

  use_frameworks!
  install_pods
  
  target 'PostLikeTests' do
    pod 'Firebase'
    pod 'RxBlocking', '6.5.0'
    pod 'RxTest', '6.5.0'
  end
  

 end  
 












    





